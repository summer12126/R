cv <- c('A', "B", 'C')
bv <- c(TRUE, FALSE, TRUE)
fv <- c(3.4, 2.5, 8)
t <- c(1, 2, 3, 4)
icv <- c(iv, cv, bv)


rm(list())
# , int [1:5] 1~5까지 정수배열
v1 = seq(1,5)  #v11 = 1:5 

# 1~10 중 2개씩 건너 뛴 정수 배열
v2 = seq(1, 10, 2) #1 3 5 7 9

# 1~10 중 같은 간격 숫자 4개를 만들어라.
v3 = seq(1, 10, length=4) #1 4 7 10

# 1~10 중 같은 간격 숫자 5개 만들어라.
v4 = seq(1, 3, lengt=5) #1 1.5 2 2.5 3

rm(list=ls())
v1 = rep(1,5)
v2 = rep(1, time=5)
